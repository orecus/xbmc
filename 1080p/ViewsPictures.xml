<includes>
  <include name="PictureWrapView">
    <control type="group">
      <visible>Control.IsVisible(510)</visible>
      <include>VisibleFadeEffect</include>
      <posx>0</posx>
      <posy>660</posy>
      <control type="wraplist" id="510">
        <posx>-38</posx>
        <posy>8</posy>
        <width>1995</width>
        <height>375</height>
        <onleft>510</onleft>
        <onright>510</onright>
        <onup>2</onup>
        <ondown>60</ondown>
        <viewtype label="31032">wrap</viewtype>
        <pagecontrol>60</pagecontrol>
        <scrolltime>200</scrolltime>
        <focusposition>3</focusposition>
        <orientation>horizontal</orientation>
        <preloaditems>4</preloaditems>
        <itemlayout height="300" width="285">
          <control type="image">
            <posx>22</posx>
            <posy>60</posy>
            <width>240</width>
            <height>240</height>
            <aspectratio>stretch</aspectratio>
            <bordertexture border="10">ThumbShadow.png</bordertexture>
            <bordersize>8</bordersize>
            <fadetime>200</fadetime>
            <texture border="2">ThumbBG.png</texture>
          </control>
          <control type="image">
            <posx>38</posx>
            <posy>75</posy>
            <width>210</width>
            <height>210</height>
            <aspectratio>keep</aspectratio>
            <texture background="true">$INFO[ListItem.Icon]</texture>
          </control>
        </itemlayout>
        <focusedlayout height="300" width="285">
          <control type="image">
            <posx>22</posx>
            <posy>60</posy>
            <width>240</width>
            <height>240</height>
            <aspectratio>stretch</aspectratio>
            <bordertexture border="10">ThumbShadow.png</bordertexture>
            <bordersize>8</bordersize>
            <fadetime>200</fadetime>
            <texture border="2">ThumbBG.png</texture>
            <animation reversible="false" effect="zoom" start="22,60,240,240" end="-15,-8,315,315" time="200">focus</animation>
            <animation reversible="false" effect="zoom" end="22,60,240,240" start="-15,-8,315,315" time="200">unfocus</animation>
          </control>
          <control type="image">
            <posx>38</posx>
            <posy>75</posy>
            <width>210</width>
            <height>210</height>
            <aspectratio>keep</aspectratio>
            <texture background="true">$INFO[ListItem.Icon]</texture>
            <animation reversible="false" effect="zoom" start="38,75,210,210" end="8,15,270,270" time="200">focus</animation>
            <animation reversible="false" effect="zoom" end="38,75,210,210" start="8,15,270,270" time="200">unfocus</animation>
          </control>
        </focusedlayout>
      </control>
      <control type="scrollbar" id="60">
        <posx>465</posx>
        <posy>300</posy>
        <width>990</width>
        <height>38</height>
